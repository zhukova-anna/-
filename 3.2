#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main()
{
    srand(time(NULL));
    int i,j,n,m,min=100;
    printf("n= ");
    scanf("%d",&n);
    while(n<0){
        printf("Try again");
        scanf("%d",&n);
    }
    printf("m= ");
    scanf("%d",&m);
    while(m<0){
        printf("Try again");
        scanf("%d",&m);
    }
    int RR[n][m];
    for(i=0;i<n;i++)
        for(j=0;j<m;j++){
            RR[i][j]=-20+rand()%(20+20+1);
    }
    for(i=0;i<n;i++){
        printf("\n");
        for(j=0;j<m;j++)
            printf("%3d ",RR[i][j]);
    }
    int sum=0,k=0,ser=0;
    for(i=0;i<n;i++){
            sum=0;
            k=0;
    for(j=0;j<m;j++){
            if(RR[i][j]>0){
                sum+=RR[i][j];
                k++;
            }
    }
        ser=sum/k;
        if(ser<min)
            min=ser;
    }
    printf("\nmin=%d",min);
    return 0;
}
