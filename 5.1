#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <locale.h>

int main() {
    srand(time(NULL));
    int n,i,j;
    printf("n= ");
    scanf("%d",&n);
    while(n<0){
        printf("Try again");
        scanf("%d",&n);
    }
    int A[n][n];
    int min,k=0;
    for(i=0;i<n;i++){
        for(j=0;j<n;j++)
        A[i][j]=rand()%20;
    }

    for(i=0;i<n;i++){
        printf("\n");
        for(j=0;j<n;j++)
            printf("%2d ",A[i][j]);
    }

    int b[100];
    int h;
    for(i=0;i<n;i++){
            min=A[i][0];
            k=0;
    for(j=0;j<n;j++){
            if(A[i][j]<min){
                min=A[i][j];
                k=j;
            }
    }

    b[i*n+j]=k;
    printf("\n%d",b[i*n+j]);
    }

    return 0;
}
