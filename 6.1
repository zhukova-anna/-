#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <locale.h>

int main()
{
    setlocale(LC_ALL, "");
    int i,j,n,m;
    printf("n= ");
    scanf("%d",&n);
    while(n<0){
        printf("Try again");
        scanf("%d",&n);
    }
    printf("m= ");
    scanf("%d",&m);
    while(m<0){
        printf("Try again");
        scanf("%d",&m);
    }
    int M[n][m];
    srand(time(NULL));
    for(i=0;i<n;i++)
        for(j=0;j<m;j++)
        M[i][j]=10*i+rand()%(50-10*i+1);

    for(i=0;i<n;i++){
            printf("\n");
        for(j=0;j<m;j++)
            printf("%2d ",M[i][j]);
    }
    int p;
    printf("p= ");
    scanf("%d",&p);
    printf("Рейтинг бильше р: ");
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            if(M[i][j]>p)
                printf("%d ",j);
        }
    }
    return 0;
}
